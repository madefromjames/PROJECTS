{
    "version": 2,
    "builds": [
      {
        "src": "commerce/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "zeroConfig": true
        }
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "commerce/wsgi.py"
      }
    ]
  }
  